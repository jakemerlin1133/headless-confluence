{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Content": {
      "type": "richtext"
    },
    "SEO_Title": {
      "type": "string"
    },
    "SEO_Description": {
      "type": "string"
    },
    "Excerpt": {
      "type": "string"
    },
    "Category": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "Article"
    },
    "FAQS": {
      "type": "component",
      "component": "shared.faqs",
      "repeatable": false
    },
    "service": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service.service",
      "inversedBy": "parents"
    }
  }
}
