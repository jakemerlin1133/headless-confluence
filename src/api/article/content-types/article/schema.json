{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Summary": {
      "type": "string"
    },
    "Content": {
      "type": "blocks"
    },
    "Cover_Image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "aiMeta": {
      "type": "json"
    },
    "seoTitle": {
      "type": "string"
    },
    "seoDescription": {
      "type": "string"
    },
    "Category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "Tag": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tag.tag"
    }
  }
}
