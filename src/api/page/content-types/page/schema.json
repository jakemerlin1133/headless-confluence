{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Slug": {
      "type": "uid"
    },
    "seoTitle": {
      "type": "string",
      "maxLength": 60
    },
    "seoDescription": {
      "type": "string",
      "maxLength": 160
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "sections.hero",
        "sections.content",
        "sections.faqs",
        "sections.testimonials",
        "sections.services-list",
        "sections.team"
      ]
    },
    "service_child_pages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service-child-page.service-child-page",
      "mappedBy": "parent_service"
    }
  }
}
